{
  "name": "wargabantuwarga.com",
  "version": "0.1.0",
  "scripts": {
    "build": "next build",
    "dev": "next",
    "fetch-wbw": "node etc/fetchers/fetch-wbw.js",
    "lint": "next lint",
    "netlify-export": "yarn fetch-wbw && yarn build && next export",
    "prepare": "husky install",
    "start": "next start"
  },
  "dependencies": {
    "@netlify/plugin-nextjs": "3.6.2",
    "cheerio": "^1.0.0-rc.10",
    "next": "11.0.1",
    "node-fetch": "^2.6.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.13",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "husky": ">=6",
    "lint-staged": ">=10",
    "netlify-plugin-cache-nextjs": "1.6.1",
    "netlify-plugin-image-optim": "0.4.0",
    "prettier": "2.3.2",
    "typescript": "4.3.5"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write"
    ],
    "**/**.css": [
      "prettier --write"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "private": true
}
